{
  "apps" : [{
    "name"        : "wot",
    "script"      : "server.js",
    "log_date_format"  : "YYYY-MM-DD HH:mm Z",
    "log_file"   : "../logs/wot.log",
    "error_file" : "../logs/wot-err.log",
    "out_file"   : "../logs/wot-out.log",
    "pid_file"   : "../logs/wot.pid",
    "ignoreWatch" : ["node_modules"],
    "watch"       : true,
    "cwd"         : "/vagrant/wot",
    "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "/vagrant/wot"
    }
  }]
}
